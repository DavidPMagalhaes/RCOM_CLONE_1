    "version": "0.2.0",
    "configurations": [
        {
            "name": "Application Receiver",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/app.exe",
            "args": ["11"],
            "environment": [{ "name": "config", "value": "Debug" }],
            "cwd": "${workspaceFolder}",
            "miDebuggerPath": "/usr/bin/gdb",
            "stopAtConnect": true
        },
        {
            "name": "Application Transmitter",
            "type": "cppdbg",
            "request": "launch",
